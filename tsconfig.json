{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "ES2022"
    ],
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": false,
    "noEmit": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noFallthroughCasesInSwitch": false,
    "strictFunctionTypes": false,
    "strictBindCallApply": false,
    "strictPropertyInitialization": false,
    "alwaysStrict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "exactOptionalPropertyTypes": false,
    "noUncheckedIndexedAccess": false,
    "noImplicitOverride": false,
    "useUnknownInCatchVariables": false,
    "allowUnusedLabels": true,
    "allowUnreachableCode": true,
    "forceConsistentCasingInFileNames": false,
    "noErrorTruncation": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "react",
    "incremental": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": [
      "vitest/globals",
      "@vitest/ui",
      "node",
      "react",
      "react-dom"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./"
      ],
      "@/src/*": [
        "./src/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "types/**/*.ts",
    "types/react.d.ts"
  ],
  "exclude": [
    "node_modules",
    "tests",
    "playwright-report",
    "test-results",
    "app/api/alerts/instances/[id]/route.ts",
    "app/api/alerts/system/status/route.ts",
    "app/api/cost-monitoring/stats/route.ts",
    "app/api/execution-history/route.ts",
    "app/api/monitoring/performance-metrics/route.ts",
    "app/api/monitoring/trading-analytics/route.ts",
    "app/api/snipe-targets/[id]/route.ts",
    "app/api/snipe-targets/route.ts",
    "app/api/snipe-targets/test/route.ts",
    "app/api/test-users/route.ts",
    "app/api/transactions/route.ts",
    "app/api/trading-settings/route.ts",
    "app/api/triggers/multi-phase-strategy/route.ts",
    "app/api/tuning/emergency-stop/route.ts",
    "app/api/tuning/optimization-history/route.ts",
    "src/test-utils/database-helpers.ts",
    "src/services/trading/mexc-trading-service.ts",
    "src/services/trading/orchestrator/types.ts",
    "src/services/trading/portfolio-tracking-service.ts",
    "src/services/trading/strategy-initialization-service.ts",
    "src/services/trading/integrated-trading-dashboard.ts",
    "app/api/tuning/optimizations/route.ts",
    "app/api/tuning/parameters/route.ts",
    "app/api/tuning/performance-metrics/route.ts",
    "app/api/user-preferences/route.ts",
    "app/api/workflow-executions/route.ts",
    "app/monitoring/page.tsx",
    "scripts/manage-test-branches.ts",
    "scripts/test-account-balance-fixes.ts",
    "scripts/test-active-targets.ts",
    "src/core/pattern-detection/confidence-calculator.ts",
    "src/core/pattern-detection/pattern-storage.ts",
    "src/services/trading/consolidated/core-trading/utils/database-operations.ts"
  ],
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs",
      "strict": false,
      "skipLibCheck": true,
      "allowJs": true
    }
  }
}
