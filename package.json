{
  "name": "mexc-sniper-bot",
  "version": "0.1.0",
  "description": "AI-powered MEXC cryptocurrency trading bot with pattern detection and automated execution",
  "scripts": {
    "postinstall": "echo 'Dependencies installed'",
    "dev": "next dev -p 3008",
    "build": "next build",
    "build:verify": "bun run type-check && bun run lint && bun run build",
    "start": "next start",
    "lint": "bun x biome check --write",
    "type-check": "tsc --noEmit",
    "test": "TEST_TYPE=unit vitest run --config=vitest.config.unified.ts",
    "test:optimized": "TEST_TYPE=unit vitest run --config=vitest.config.unified.ts",
    "test:ultra-fast": "TEST_TYPE=unit vitest run --config=vitest.config.unified.ts --run",
    "test:cached": "TEST_TYPE=unit vitest run --config=vitest.config.unified.ts --cache.dir=.vitest-cache",
    "test:fast:unit": "TEST_TYPE=unit vitest run --config=vitest.config.unified.ts",
    "test:fast:integration": "TEST_TYPE=integration vitest run --config=vitest.config.unified.ts",
    "test:fast:parallel": "TEST_TYPE=unit vitest run --config=vitest.config.unified.ts --maxConcurrency=16",
    "test:fast": "TEST_TYPE=unit vitest run --config=vitest.config.unified.ts",
    "test:integration": "TEST_TYPE=integration vitest run --config=vitest.config.unified.ts",
    "test:e2e": "vitest run --config=vitest.config.e2e.ts",
    "test:performance": "TEST_TYPE=performance vitest run --config=vitest.config.unified.ts",
    "test:all": "bun run test && bun run test:integration && bun run test:e2e && playwright test",
    "test:watch": "TEST_TYPE=unit vitest --config=vitest.config.unified.ts",
    "test:coverage": "rm -rf coverage && COVERAGE=true TEST_TYPE=unit vitest run --config=vitest.config.unified.ts",
    "test:stagehand": "playwright test tests/e2e/stagehand-*.spec.ts",
    "test:security": "TEST_TYPE=stability vitest run --config=vitest.config.unified.ts",
    "test:build-verify": "bun run type-check && bun run lint && bun run test",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run scripts/database-migration-runner.ts",
    "db:migrate:verbose": "bun run scripts/database-migration-runner.ts --verbose",
    "db:setup": "bun run scripts/setup-database.ts",
    "db:setup:verbose": "bun run scripts/setup-database.ts --verbose",
    "db:studio": "drizzle-kit studio",
    "db:reset": "drizzle-kit drop && bun run db:migrate"
  },
