{
  "name": "mexc-sniper-bot",
  "version": "0.1.0",
  "description": "AI-powered MEXC cryptocurrency trading bot with pattern detection and automated execution",
  "scripts": {
    "postinstall": "echo 'Build dependencies installed successfully'",
    "dev": "next dev -p 3008",
    "dev:turbo": "next dev --turbopack -p 3008",
    "build": "DISABLE_TELEMETRY=true next build",
    "start": "next start",
    "lint": "biome check --write",
    "lint:check": "biome check",
    "format": "biome format --write",
    "format:check": "biome format",
    "lint:all": "./scripts/lint-and-format.sh",
    "type-check": "tsc --noEmit",
    "test:mexc-credentials": "tsx scripts/test-mexc-credentials.ts",
    "// ===== UNIFIED TESTING FRAMEWORK =====": "",
    "test": "vitest run --config=vitest.config.unified.js",
    "test:unit": "vitest run tests/unit/ --config=vitest.config.unified.js",
    "test:integration": "vitest run tests/integration/ --config=vitest.config.unified.js",
    "test:watch": "vitest --watch --config=vitest.config.unified.js",
    "test:ui": "vitest --ui --config=vitest.config.unified.js",
    "test:coverage": "vitest run --coverage --config=vitest.config.unified.js",
    "// ===== AI-POWERED E2E TESTING =====": "",
    "test:e2e": "playwright test tests/e2e/stagehand-*.spec.ts --config=stagehand.config.unified.ts",
    "test:e2e:auth": "playwright test tests/e2e/stagehand-auth-complete.spec.ts --config=stagehand.config.unified.ts",
    "test:e2e:dashboard": "playwright test tests/e2e/stagehand-dashboard-enhanced.spec.ts --config=stagehand.config.unified.ts",
    "test:e2e:journey": "playwright test tests/e2e/stagehand-user-journey.spec.ts --config=stagehand.config.unified.ts",
    "test:e2e:headed": "STAGEHAND_HEADLESS=false playwright test tests/e2e/stagehand-*.spec.ts --config=stagehand.config.unified.ts",
    "test:e2e:debug": "STAGEHAND_VERBOSE=true STAGEHAND_DEBUG_DOM=true playwright test tests/e2e/stagehand-*.spec.ts --config=stagehand.config.unified.ts",
    "// ===== AUTHENTICATION E2E TESTING =====": "",
    "test:e2e:auth:validation": "playwright test tests/e2e/auth-flow-validation.spec.ts",
    "test:e2e:auth:api": "playwright test tests/e2e/auth-api-validation.spec.ts",
    "test:e2e:auth:all": "playwright test tests/e2e/auth-*.spec.ts",
    "test:e2e:auth:chrome": "playwright test tests/e2e/auth-flow-validation.spec.ts --project=auth-chrome",
    "test:e2e:auth:mobile": "playwright test tests/e2e/auth-flow-validation.spec.ts --project=auth-mobile",
    "test:e2e:auth:report": "playwright show-report playwright-report",
    "// ===== DEPLOYMENT VALIDATION =====": "",
    "test:deployment:staging": "TEST_ENVIRONMENT=staging TEST_BASE_URL=https://staging.mexcsniper.com npm run test:e2e:auth:all",
    "test:deployment:production": "TEST_ENVIRONMENT=production TEST_BASE_URL=https://mexcsniper.com npm run test:e2e:auth:validation",
    "// ===== LEGACY E2E TESTING =====": "",
    "test:e2e:legacy": "playwright test tests/e2e/ --ignore=tests/e2e/stagehand-*.spec.ts --ignore=tests/e2e/auth-*.spec.ts",
    "test:e2e:legacy:ui": "playwright test tests/e2e/ --ignore=tests/e2e/stagehand-*.spec.ts --ignore=tests/e2e/auth-*.spec.ts --ui",
    "test:e2e:legacy:report": "playwright show-report",
    "// ===== COMPREHENSIVE TEST SUITES =====": "",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:all:parallel": "npm run test & npm run test:e2e & wait",
    "test:smoke": "npm run test:unit && npm run test:e2e:auth",
    "test:full": "npm run test:coverage && npm run test:e2e && npm run test:e2e:legacy",
    "// ===== CI/CD OPTIMIZED =====": "",
    "test:ci": "vitest run --reporter=github-actions --coverage --config=vitest.config.unified.js",
    "test:e2e:ci": "CI=true playwright test tests/e2e/stagehand-*.spec.ts --reporter=github --config=stagehand.config.unified.ts",
    "test:ci:full": "npm run test:ci && npm run test:e2e:ci",
    "// ===== PERFORMANCE & BENCHMARKING =====": "",
    "test:perf": "vitest run --reporter=verbose --logHeapUsage --config=vitest.config.unified.js",
    "test:benchmark": "vitest bench --config=vitest.config.unified.js",
    "// ===== BUNDLE OPTIMIZATION & ANALYSIS =====": "",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "build:analyze": "npm run analyze && npm run build:production",
    "// ===== DEVELOPMENT & MAINTENANCE =====": "",
    "pre-commit": "bun run lint:all && bun run type-check && bun run test:smoke",
    "ci:quality": "bun run lint:check && bun run format:check && bun run type-check",
    "ci:test": "bun run test:ci:full",
    "ci:build": "bun run build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:safe": "./scripts/safe-migrate.sh",
    "db:migrate:fk": "bun run src/db/migrations/safe-foreign-key-migration.ts",
    "db:verify:fk": "bun run src/db/migrations/verify-foreign-keys.ts",
    "db:studio": "drizzle-kit studio",
    "db:reset": "echo 'Database reset not available for PostgreSQL. Use NeonDB console for database management.'",
    "db:check": "tsx scripts/check-db.ts",
    "db:diagnostic": "tsx scripts/database-health-diagnostic.ts",
    "db:optimize": "echo 'Database optimization handled automatically by NeonDB PostgreSQL.'",
    "db:setup:neon": "echo 'NeonDB setup: Configure DATABASE_URL environment variable'",
    "db:test:neon": "npx tsx scripts/test-neon-connection.ts",
    "// ===== NEONDB BRANCH MANAGEMENT =====": "",
    "branch:create": "tsx scripts/manage-test-branches.ts create",
    "branch:list": "tsx scripts/manage-test-branches.ts list",
    "branch:delete": "tsx scripts/manage-test-branches.ts delete",
    "branch:cleanup": "tsx scripts/manage-test-branches.ts cleanup",
    "branch:connection": "tsx scripts/manage-test-branches.ts connection",
    "branch:health": "tsx scripts/manage-test-branches.ts health",
    "branch:migrate": "tsx scripts/manage-test-branches.ts migrate",
    "branch:help": "tsx scripts/manage-test-branches.ts help",
    "branch:setup": "tsx scripts/setup-branch-testing.ts",
    "branch:setup:validate": "tsx scripts/setup-branch-testing.ts --validate",
    "branch:setup:test": "tsx scripts/setup-branch-testing.ts --test",
    "// ===== AGENT & TESTING =====": "",
    "agents:test": "bun test src/mexc-agents/**/*.test.ts",
    "agents:health": "bun run src/mexc-agents/health-check.ts",
    "// ===== CIRCUIT BREAKER & SAFETY =====": "",
    "fix-circuit-breaker": "tsx scripts/fix-circuit-breaker.ts",
    "safety:diagnose": "tsx scripts/fix-circuit-breaker.ts diagnose",
    "safety:fix": "tsx scripts/fix-circuit-breaker.ts fix",
    "safety:validate": "tsx scripts/fix-circuit-breaker.ts validate",
    "safety:check": "tsx scripts/fix-circuit-breaker.ts check"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^2.3.0",
    "@kinde-oss/kinde-auth-nextjs": "^2.7.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.56.1",
    "@opentelemetry/exporter-jaeger": "^1.26.0",
    "@opentelemetry/exporter-prometheus": "^0.202.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-metrics": "^1.26.0",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@opentelemetry/sdk-trace-node": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "@opentelemetry/winston-transport": "^0.13.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-query-devtools": "^5.80.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "inngest": "^3.38.0",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.514.0",
    "next": "15.3.3",
    "openai": "^5.1.1",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.64"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@next/bundle-analyzer": "^15.3.3",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.2",
    "autoprefixer": "^10.4.21",
    "critters": "^0.0.23",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "null-loader": "^4.0.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "tailwindcss": "^4.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.2"
  },
  "optionalDependencies": {
    "@tailwindcss/oxide-linux-x64-gnu": "^4.0.14",
    "lightningcss-linux-x64-gnu": "^1.29"
  },
  "overrides": {
    "esbuild": "^0.25.5",
    "@esbuild-kit/core-utils": {
      "esbuild": "^0.25.5"
    },
    "drizzle-kit": {
      "@esbuild-kit/esm-loader": {
        "@esbuild-kit/core-utils": {
          "esbuild": "^0.25.5"
        }
      }
    },
    "@opentelemetry/auto-instrumentations-node": "^0.56.1",
    "@opentelemetry/core": "^1.25.1",
    "@opentelemetry/api": "^1.9.0"
  }
}
