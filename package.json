{
  "name": "mexc-sniper-bot",
  "version": "0.1.0",
  "description": "AI-powered MEXC cryptocurrency trading bot with pattern detection and automated execution",
  "scripts": {
    "postinstall": "echo 'Dependencies installed'",
    "dev": "next dev -p 3008",
    "build": "NODE_OPTIONS='--max-old-space-size=2048' next build",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=1024' next build",
    "build:analyze": "ANALYZE=true NODE_OPTIONS='--max-old-space-size=2048' next build",
    "build:performance": "NODE_OPTIONS='--max-old-space-size=2048' next build",
    "build:verify": "bun run type-check:fast && bun run lint:fast && bun run build",
    "start": "next start",
    "lint": "bun x biome check --write",
    "lint:fast": "bun x biome check --write --no-errors-on-unmatched",
    "lint:strict": "bun x biome check --config-path biome.strict.json",
    "type-check": "tsc --noEmit",
    "type-check:fast": "tsc --noEmit --skipLibCheck --assumeChangesOnlyAffectDirectDependencies",
    "test": "TEST_TYPE=unit vitest run --config=vitest.config.ts",
    "test:integration": "TEST_TYPE=integration vitest run --config=vitest.config.ts",
    "test:e2e": "TEST_TYPE=e2e vitest run --config=vitest.config.ts",
    "test:performance": "TEST_TYPE=performance vitest run --config=vitest.config.ts",
    "test:performance:fast": "TEST_TYPE=performance PERF_MODE=fast vitest run --config=vitest.config.ts",
    "test:performance:ultra": "TEST_TYPE=performance PERF_MODE=ultra vitest run --config=vitest.config.ts",
    "test:all": "bun run test && bun run test:integration && bun run test:e2e && playwright test",
    "test:watch": "TEST_TYPE=unit vitest --config=vitest.config.ts",
    "test:coverage": "rm -rf coverage && COVERAGE=true TEST_TYPE=unit vitest run --config=vitest.config.ts",
    "test:stagehand": "playwright test tests/e2e/stagehand-*.spec.ts",
    "test:stagehand:auth": "bun run scripts/run-stagehand-auth-tests.ts",
    "test:stagehand:user-creation": "playwright test tests/e2e/stagehand-user-creation.spec.ts",
    "test:stagehand:comprehensive": "playwright test tests/e2e/stagehand-auth-comprehensive.spec.ts",
    "demo:stagehand": "npx tsx scripts/demo-stagehand-auth-node.ts",
    "verify:stagehand": "bun run scripts/verify-stagehand-setup.ts",
    "test:stability": "TEST_TYPE=stability vitest run --config=vitest.config.ts",
    "test:supabase": "TEST_TYPE=supabase vitest run --config=vitest.config.ts",
    "test:sync": "TEST_TYPE=sync vitest run --config=vitest.config.ts",
    "test:build-verify": "bun run type-check && bun run lint && bun run test",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run scripts/database-migration-runner.ts",
    "db:migrate:verbose": "bun run scripts/database-migration-runner.ts --verbose",
    "db:setup": "bun run scripts/setup-database.ts",
    "db:setup:verbose": "bun run scripts/setup-database.ts --verbose",
    "db:studio": "drizzle-kit studio",
    "db:reset": "drizzle-kit drop && bun run db:migrate",
    "deps:check": "npm outdated",
    "deps:audit": "npm audit --audit-level=moderate",
    "deps:update": "npm update"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^2.4.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.56.1",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/exporter-prometheus": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.3",
    "@tanstack/react-query": "^5.81.5",
    "@types/node-fetch": "^2.6.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "inngest": "^3.40.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "node-fetch": "^3.3.2",
    "openai": "4.67.3",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.4",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zod": "^3.25.74"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@next/bundle-analyzer": "^15.3.5",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/ui": "^3.2.4",
    "istanbul-lib-coverage": "^3.2.1",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "esbuild": "^0.25.5"
  }
}
