{
  "name": "mexc-sniper-bot",
  "version": "0.1.0",
  "description": "AI-powered MEXC cryptocurrency trading bot with pattern detection and automated execution",
  "scripts": {
    "postinstall": "echo 'Dependencies installed'",
    "dev": "next dev -p 3008",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --write",
    "type-check": "tsc --noEmit",
    "test": "vitest run --config=vitest.config.unified.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:stagehand": "playwright test tests/e2e/stagehand-*.spec.ts",
    "test:stagehand:auth": "playwright test tests/e2e/stagehand-auth-*.spec.ts",
    "test:stagehand:dashboard": "playwright test tests/e2e/stagehand-dashboard*.spec.ts",
    "test:stagehand:patterns": "playwright test tests/e2e/stagehand-pattern-discovery.spec.ts",
    "test:stagehand:api": "playwright test tests/e2e/stagehand-api-trading.spec.ts",
    "test:stagehand:integration": "playwright test tests/e2e/stagehand-*.spec.ts",
    "test:stagehand:headed": "playwright test tests/e2e/stagehand-*.spec.ts --headed",
    "test:stagehand:ui": "playwright test tests/e2e/stagehand-*.spec.ts --ui",
    "test:fast": "vitest run --config=vitest.config.unified.ts",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:fast:unit": "vitest run tests/unit/",
    "test:fast:integration": "vitest run tests/integration/",
    "test:fast:parallel": "vitest run --config=vitest.config.unified.ts --reporter=basic",
    "test:emergency": "vitest run --config=vitest.config.unified.ts --reporter=basic --run"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^2.4.0",
