{
  "name": "mexc-sniper-bot",
  "version": "0.1.0",
  "description": "AI-powered MEXC cryptocurrency trading bot with pattern detection and automated execution",
  "scripts": {
    "postinstall": "echo 'Dependencies installed'",
    "dev": "next dev -p 3008",
    "build": "next build",
    "build:verify": "bun run type-check && bun run lint && bun run build",
    "start": "next start",
    "lint": "bun x biome check --write",
    "type-check": "tsc --noEmit",
    "test": "TEST_TYPE=unit vitest run --config=vitest.config.master.ts",
    "test:stability": "TEST_TYPE=stability vitest run --config=vitest.config.master.ts",
    "test:stability:verbose": "TEST_TYPE=stability vitest run --config=vitest.config.master.ts --reporter=verbose",
    "test:performance": "TEST_TYPE=performance vitest run --config=vitest.config.master.ts",
    "test:supabase": "TEST_TYPE=supabase vitest run --config=vitest.config.master.ts",
    "test:sync": "TEST_TYPE=sync vitest run --config=vitest.config.master.ts",
    "test:e2e": "playwright test",
    "test:e2e:coverage": "PW_EXPERIMENTAL_TEST_COVERAGE=1 playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:stagehand": "playwright test tests/e2e/stagehand-*.spec.ts",
    "test:stagehand:auth": "playwright test tests/e2e/stagehand-auth-*.spec.ts",
    "test:stagehand:dashboard": "playwright test tests/e2e/stagehand-dashboard*.spec.ts",
    "test:stagehand:patterns": "playwright test tests/e2e/stagehand-pattern-discovery.spec.ts",
    "test:stagehand:api": "playwright test tests/e2e/stagehand-api-trading.spec.ts",
    "test:stagehand:integration": "playwright test tests/e2e/stagehand-*.spec.ts",
    "test:stagehand:headed": "playwright test tests/e2e/stagehand-*.spec.ts --headed",
    "test:stagehand:ui": "playwright test tests/e2e/stagehand-*.spec.ts --ui",
    "test:fast": "TEST_TYPE=performance vitest run --config=vitest.config.fast.ts",
    "test:optimized": "bun run scripts/test-performance-optimizer.ts unit",
    "test:ultra-fast": "TEST_TYPE=fast vitest run --config=vitest.config.fast.ts --reporter=basic",
    "test:cached": "bun run scripts/test-cache-optimizer.ts optimize unit && TEST_TYPE=unit vitest run --config=vitest.config.optimized.ts",
    "test:unit": "TEST_TYPE=unit vitest run --config=vitest.config.master.ts",
    "test:unit:stability": "TEST_TYPE=stability vitest run --config=vitest.config.master.ts",
    "test:integration": "TEST_TYPE=integration vitest run --config=vitest.config.master.ts",
    "test:integration:enhanced": "bun run tests/utils/integration-test-runner.ts",
    "test:integration:verbose": "TEST_SERVER_LOGS=true TEST_TYPE=integration vitest run --config=vitest.config.master.ts",
    "test:integration:coverage": "COVERAGE=true TEST_TYPE=integration vitest run --config=vitest.config.master.ts",
    "test:integration:parallel": "bun run tests/utils/integration-test-runner.ts --parallel",
    "test:integration:single": "bun run tests/utils/integration-test-runner.ts --no-parallel",
    "test:integration:real-api": "TEST_TYPE=integration vitest run tests/integration/real-api-endpoints.test.ts --config=vitest.config.master.ts",
    "test:integration:database": "TEST_TYPE=integration vitest run tests/integration/database-integration.test.ts --config=vitest.config.master.ts",
    "test:integration:websocket": "TEST_TYPE=integration vitest run tests/integration/websocket-realtime.test.ts --config=vitest.config.master.ts",
    "test:integration:stability": "TEST_TYPE=stability vitest run tests/integration/ --config=vitest.config.master.ts",
    "test:fast:unit": "TEST_TYPE=performance vitest run tests/unit/ --config=vitest.config.master.ts",
    "test:fast:integration": "TEST_TYPE=performance vitest run tests/integration/ --config=vitest.config.master.ts",
    "test:fast:parallel": "TEST_TYPE=performance vitest run --config=vitest.config.master.ts --reporter=basic",
    "test:emergency": "TEST_TYPE=unit vitest run --config=vitest.config.master.ts --reporter=basic --run",
    "test:single-thread": "VITEST_SINGLE_THREAD=true TEST_TYPE=stability vitest run --config=vitest.config.master.ts",
    "test:coverage": "rm -rf coverage .nyc_output && COVERAGE=true TEST_TYPE=unit vitest run --config=vitest.config.master.ts && npm run test:e2e:coverage && node scripts/merge-coverage.js",
    "test:config:validate": "bun run scripts/test-config-validator.ts",
    "test:config:migrate": "bun run scripts/test-config-migration.ts",
    "test:config:test-all": "bun run scripts/test-all-configurations.ts",
    "test:performance:analyze": "bun run scripts/test-performance-optimizer.ts unit 1",
    "test:performance:monitor": "bun run scripts/test-performance-optimizer.ts",
    "test:cache:optimize": "bun run scripts/test-cache-optimizer.ts optimize",
    "test:cache:prewarm": "bun run scripts/test-cache-optimizer.ts prewarm",
    "test:cache:clear": "bun run scripts/test-cache-optimizer.ts clear",
    "test:cache:stats": "bun run scripts/test-cache-optimizer.ts stats",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run scripts/database-migration-runner.ts",
    "db:migrate:verbose": "bun run scripts/database-migration-runner.ts --verbose",
    "db:setup": "bun run scripts/setup-database.ts",
    "db:setup:verbose": "bun run scripts/setup-database.ts --verbose",
    "db:studio": "drizzle-kit studio",
    "db:reset": "drizzle-kit drop && bun run db:migrate"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^2.4.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.56.1",
