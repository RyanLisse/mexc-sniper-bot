    "ioredis": "^5.6.1",
    "lucide-react": "^0.522.0",
    "next": "15.3.4",
    "openai": "4.67.3",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.4",
    "sonner": "^2.0.5",
